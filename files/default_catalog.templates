catalog.RegionOne.volume.internalURL = http://%SERVICE_HOST%:%NOVA_VOLUME_PORT%/v1/$(tenant_id)s
catalog.RegionOne.volume.name = Volume Service


catalog.RegionOne.ec2.publicURL = http://%SERVICE_HOST%:%NOVA_EC2_PORT%/services/Cloud
catalog.RegionOne.ec2.adminURL = http://%SERVICE_HOST%:%NOVA_EC2_PORT%/services/Admin
catalog.RegionOne.ec2.internalURL = http://%SERVICE_HOST%:%NOVA_EC2_PORT%/services/Cloud
catalog.RegionOne.ec2.name = EC2 Service


catalog.RegionOne.s3.publicURL = http://%SERVICE_HOST%:%S3_SERVICE_PORT%
